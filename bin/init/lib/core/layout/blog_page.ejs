<h1>Blog Page</h1>

<p>page#: <%= currentPage.pageNum %></p>

<% currentPage.page.forEach((post)=>{ %>
  <p><a href="<%- '/blog' + post.dir_post + '/index.html' %>" ><%= post.title %></a ></p>
<% }); %>

<p>page#: <%= currentPage.pageNum %></p>
<% if(currentPage.pageNum > 1){ %>
  <span><a href="/blog/page/<%= currentPage.pageNum - 1 %>/index.html" >Prev</a></span>
<% } %>
<% if(currentPage.pageNum < currentPage.totalPages){ %>
  <span><a href="/blog/page/<%= currentPage.pageNum + 1 %>/index.html" >Next</a></span>
<% } %>
